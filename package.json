{
  "name": "vue-rokka-image",
  "version": "0.5.1",
  "description": "Vue 2.x component for the image processing service rokka.io",
  "repository": "https://github.com:rokka-io/vue-rokka-image",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "watch": "rollup -c -w",
    "lint": "eslint 'src/*' 'tests/*'  --ignore-pattern='tests/__snapshots__'",
    "fix": "eslint 'src/*' 'tests/*' --ignore-pattern='tests/__snapshots__' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "coveralls": "npm run test -- --coverage --coverageReporters=text-lcov | coveralls"
  },
  "files": [
    "/dist/*",
    "/src/*"
  ],
  "module": "dist/index.esm.js",
  "esnext": "src/index.js",
  "main": "dist/index.umd.min.js",
  "unpkg": "dist/index.umd.min.js",
  "jsdelivr": "dist/index.umd.min.js",
  "author": "rokka.io",
  "license": "MIT",
  "engines": {
    "node": ">8"
  },
  "devDependencies": {
    "@babel/core": "^7.7.0",
    "@babel/preset-env": "^7.7.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "coveralls": "^3.0.7",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "rollup": "^1.26.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10"
  }
}
